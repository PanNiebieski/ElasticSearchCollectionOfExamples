{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ElasticSearch": {
    "Index": {
      "Name": "missions",
      "NumberOfShards": 5,
      "NumberOfReplicas": 2,
      "RefreshInterval": -1
    },
    "Connection": {
      "AuthRequired": false,
      "UserName": "",
      "Password": "",
      "AddressEndPoint": "localhost:9201",
      "Protocol": "http",
      "EnableHttpCompression": true,
      "RequestTimeout": 60000,
      "DisableAutoProxyDetection": false
    },
    "Debug": {
      "EnableDebugMode": true,
      "PrettyJson": true,
      "DisableDirectStreaming": true
    }

  }
}
